    <div class="box" id="heading">
        <h1> <%= ListTitle %></h1>
    </div>
    <div class="box">
        <%var n = newListItem.length%>
        <%for(var i =0; i<=n; i++){%>
            <div class="item">
                <input type="checkbox" >
                <p><%=newListItem[i]%> </p>
            </div>
        <%}%>
        <form class="item" action="/" method="post">
            <input type="text" placeholder="New item" name="newitem" autocomplete="off">
            <button class="button" name="list" value="<%= ListTitle  %>" type="submit">+</button>
        </form>
    </div>
   
